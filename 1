#include<iostream>
#include<conio.h>
using namespace std;
main()
{
	int num;
	cout<<"enter a number";
	cin>>num;
	string strNumber;
	switch(num){
		case 10: strNumber="ten"; break;
		case 20: strNumber="twenty"; break;
		case 30: strNumber="thirty"; break;
		default: strNumber="unknown Number"; 
	}
	cout<<"Number is "<<strNumber;

}
